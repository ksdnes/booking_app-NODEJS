<!DOCTYPE html>
<html lang="en">

<head>
  <meta charset="UTF-8">
  <meta name="viewport" content="width=device-width, initial-scale=1.0">
  <link rel="stylesheet" href="https://stackpath.bootstrapcdn.com/bootstrap/4.5.2/css/bootstrap.min.css">
  <title>Beautiful Landing Page</title>
  <% if(typeof(customScripts)!=="undefined"){
    for(const script of customScripts){ %>
      <script src="<%script.src%>" defer></script><%}}%>
</head>
<script>
console.log(status)
</script>
<body>
  <nav class="navbar navbar-expand-lg navbar-dark bg-dark">
    <a class="navbar-brand" href="/">Logo</a>
    <button class="navbar-toggler" type="button" data-toggle="collapse" data-target="#navbarNav"
      aria-controls="navbarNav" aria-expanded="false" aria-label="Toggle navigation">
      <span class="navbar-toggler-icon"></span>
    </button>
    <div class="collapse navbar-collapse" id="navbarNav">
      <ul class="navbar-nav ml-auto">
          <% if (typeof user === 'undefined') { %>
               <li class="nav-item">
                 <a class="nav-link" href="/login">Log In</a>
               </li>
               <li class="nav-item">
                 <a class="nav-link" href="/register">Register</a>
               </li>
               <% } %>
               <% if (typeof user !== 'undefined') { %>
               <% if ( user.is_partner ===1 && user.status_mode===0) { %>
               <li class="nav-item">
                    <a href="/switchtonormal" class="nav-link">Switch to normal mode</a>
               </li>
               <% }%>
               <% if ( user.is_partner===1 && user.status_mode===1 ) { %>
               <li class="nav-item">
                    <a href="/switchtoparter" class="nav-link">Switch to Partner mode</a>
               </li>
               <% }%>
               <li class="nav-item">
                 <a class="nav-link" href="/logout">Log out</a>
               </li>
             <% } %>
        
      </ul>
    </div>
  </nav>
